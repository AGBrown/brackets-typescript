{
  "rules": {
    "class-name": true,
    "curly": true,
    "eofline": true,
    "indent": [true, 4],
    "label-position": true,
    "label-undefined": true,
    "max-line-length": [true, 140],
    "no-arg": true,
    "no-bitwise": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-key": true,
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-string-literal": true,
    "no-trailing-comma": true,
    "no-unused-variable": true,
    "no-unreachable": true,
    "one-line": [true,
        "check-open-brace",
        "check-catch",
        "check-else",
        "check-whitespace"
    ],
    "quotemark": [true, "single"],
    "radix": true,
    "semicolon": true,
    "triple-equals": [true, "allow-null-check"],
    "typedef-whitespace": [true,
        {
            "callSignature": "nospace",
            "catchClause": "nospace",
            "indexSignature": "nospace",
            "parameter": "nospace",
            "propertySignature": "nospace",
            "variableDeclarator": "nospace"
        }
    ],
    "whitespace": [true,
        "check-branch",
        "check-decl",
        "check-operator",
        "check-separator",
        "check-type"
    ]
  }
}